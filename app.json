{
  "expo": {
    "name": "Tracker",
    "slug": "tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#4338ca",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#0c0a09",
    "description": "Tracking app for your fitness journey",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0c0a09"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
